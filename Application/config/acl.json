{
  "acl": {
    "routes": {
      "public": {
        "GET": [
          {
            "route": "/api/v1/login",
            "allows": [
              "admin",
              "standard",
              "guest"
            ]
          }
        ],
        "POST": [
          {
            "route": "/api/v1/login",
            "allows": [
              "admin",
              "standard",
              "guest"
            ]
          }
        ],
        "PUT": [],
        "PATCH": [],
        "DELETE": []
      },
      "private": {
        "GET": [
          {
            "route": "/api/v1/{resourceName}",
            "allows": [
              "admin",
              "standard"
            ]
          },
          {
            "route": "/api/v1/{resourceName}/{identifier}",
            "allows": [
              "admin",
              "standard"
            ]
          }
        ],
        "POST": [
          {
            "route": "/api/v1/{resourceName}",
            "allows": [
              "admin",
              "standard"
            ]
          }
        ],
        "PUT": [
          {
            "route": "/api/v1/{resourceName}",
            "allows": [
              "admin",
              "standard"
            ]
          },
          {
            "route": "/api/v1/{resourceName}/{identifier}",
            "allows": [
              "admin",
              "standard"
            ]
          }
        ],
        "PATCH": [
          {
            "route": "/api/v1/{resourceName}/{identifier}",
            "allows": [
              "admin",
              "standard"
            ]
          }
        ],
        "DELETE": [
          {
            "route": "/api/v1/{resourceName}/{identifier}",
            "allows": [
              "admin",
              "standard"
            ]
          }
        ]
      }
    },
    "roles": {
      "admin": {
        "permissions": {}
      },
      "standard": {
        "permissions": {}
      },
      "guest": {
        "permissions": {}
      }
    }
  }
}
